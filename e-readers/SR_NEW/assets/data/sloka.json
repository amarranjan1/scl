[
{
 "chaptno" : "01",
 "slokano" : "001",
 "spart1" : "तपःस्वाध्यायनिरतं तपस्वी वाग्विदां वरम्",
 "spart2" : "नारदं परिपप्रच्छ  वाल्मीकिर्मुनिपुङ्गवम्"
},

{
 "chaptno" : "01",
 "slokano" : "002",
 "spart1" : "कोन्वस्मिन् साम्प्रतं लोके गुणवान् कश्च वीर्यवान्",
"spart2" : "धर्मज्ञश्च कृतज्ञश्च सत्यवाक्यो दृढव्रतः"
},

{
 "chaptno" : "01",
 "slokano" : "003",
 "spart1" : "चारित्रेण च को युक्तः सर्वभूतेषु को हितः",
"spart2" : "विद्वान् कः कः समर्थश्च कश्चैकप्रियदर्शनः"
},

{
 "chaptno" : "01",
 "slokano" : "004",
 "spart1" : "आत्मवान् को जितक्रोधो द्युतिमान् कोऽनसूयकः",
"spart2" : "कस्य बिभ्यति देवाश्च जातरोषस्य संयुगे"
},

{
 "chaptno" : "01",
 "slokano" : "005",
 "spart1" : "एतदिच्छाम्यहं श्रोतुं परं कौतूहलं हि मे ",
"spart2" : "महर्षे त्वं समर्थोऽसि ज्ञातुमेवंविधं नरम्"
},

{
 "chaptno" : "01",
 "slokano" : "006",
 "spart1" : "श्रुत्वा  चैतत्त्रिलोकज्ञो वाल्मीकेर्नारदो वचः",
"spart2" : "श्रूयतामिति चामन्त्र्य प्रहृष्टो वाक्यमब्रवीत्"
},

{
 "chaptno" : "01",
 "slokano" : "007",
 "spart1" : "बहवो दुर्लभाश्चैव ये त्वया कीर्तिता गुणाः",
"spart2" : "मुने वक्ष्याम्यहं बुद्ध्वा तैर्युक्तः श्रूयतां नरः"
},

{
 "chaptno" : "01",
 "slokano" : "008",
 "spart1" : "इक्ष्वाकुवंशप्रभवो रामो नाम जनैः श्रुतः",
"spart2" : "नियतात्मा महावीर्यो द्युतिमान् धृतिमान् वशी"
},

{
 "chaptno" : "01",
 "slokano" : "009",
 "spart1" : "बुद्धिमान् नीतिमान् वाग्ग्मी श्रीमाञ्छत्रुनिबर्हणः",
"spart2" : "विपुलांसो महाबाहुः कम्बुग्रीवो महाहनुः "
},

{
 "chaptno" : "01",
 "slokano" : "010",
 "spart1" : "महोरस्को महेष्वासो गूढजत्रुररिन्दमः",
"spart2" : "आजानुबाहुः सुशिराः सुललाटः सुविक्रमः"
},

{
 "chaptno" : "02",
 "slokano" : "011",
 "spart1" : "समः समविभक्ताङ्गः स्निग्धवर्णः प्रतापवान् ",
"spart2" : "पीनवक्षा विशालाक्षो लक्ष्मीवाञ्छुभलक्षणः"
},

{
 "chaptno" : "02",
 "slokano" : "012",
 "spart1" : "धर्मज्ञः सत्यसन्धश्च प्रजानां च हिते रतः",
"spart2" : "यशस्वी ज्ञानसम्पन्नः शुचिर्वश्यः समाधिमान्"
},

{
 "chaptno" : "02",
 "slokano" : "013",
 "spart1" : "प्रजापतिसमः श्रीमान् धाता रिपुनिषूदनः",
"spart2" : "रक्षिता जीवलोकस्य धर्मस्य परिरक्षिता"
},

{
 "chaptno" : "02",
 "slokano" : "014",
 "spart1" : "रक्षिता स्वस्य धर्मस्य स्वजनस्य च रक्षिता",
"spart2" : "वेदवेदाङ्गतत्वज्ञो धनुर्वेदे च निष्ठितः"
},

{
 "chaptno" : "02",
 "slokano" : "015",
 "spart1" : "सर्वशास्त्रार्थतत्त्वज्ञः स्मृतिमान् प्रतिभानवान्",
"spart2" : "सर्वलोकप्रियः साधुरदीनात्मा विचक्षणः"
},

{
 "chaptno" : "02",
 "slokano" : "016",
 "spart1" : "सर्वदाभिगतः सद्भिः समुद्र इव सिन्धुभिः",
"spart2" : "आर्यः सर्वसमश्चैव सदैव प्रियदर्शनः"
},

{
 "chaptno" : "02",
 "slokano" : "017",
 "spart1" : "स च सर्वगुणोपेतः कौसल्यानन्दवर्धनः",
"spart2" : "समुद्र इव गाम्भीर्ये धैर्येण हिमवानिव"
},

{
 "chaptno" : "02",
 "slokano" : "018",
 "spart1" : "विष्णुना सदृशो वीर्ये सोमवत् प्रियदर्शनः",
"spart2" : "कालाग्निसदृशः क्रोधे क्षमया पृथिवीसमः"
},

{
 "chaptno" : "02",
 "slokano" : "019",
 "spart1" : "धनदेन समस्त्यागे ",
 "spart2" : "सत्ये धर्म इवापरः"
},

{
 "chaptno" : "02",
 "slokano" : "019_3-21_1",
 "spart1" : "तमेवंगुणसम्पन्नं रामं सत्यपराक्रमम् ज्येष्ठं ज्येष्ठगुणैर्युक्तं प्रियं दशरथः सुतम्। ",
 "spart2" : "प्रकृतीनां हितैर्युक्तं प्रकृतिप्रियकाम्यया।  यौवराज्येन संयोक्तुमैच्छत् प्रीत्या महीपतिः"
},

{
 "chaptno" : "03",
 "slokano" : "021_2-22_1",
 "spart1" : "तस्याभिषेकसम्भारान् दृष्ट्वा भार्याऽथ कैकयी",
"spart2" : "पूर्वं दत्तवरा देवी वरमेनमयाचत। "
},

{
 "chaptno" : "03",
 "slokano" : "022_2",
 "spart1" : "विवासनं च रामस्य भरतस्याभिषेचनम्"
},

{
 "chaptno" : "03",
 "slokano" : "023",
 "spart1" : "स सत्यवचनाद्राजा धर्मपाशेन संयतः",
"spart2" : "विवासयामास सुतं रामं दशरथः प्रियम्"
},

{
 "chaptno" : "03",
 "slokano" : "024",
 "spart1" : "स जगाम वनं वीरः प्रतिज्ञामनुपालयन्",
"spart2" : "पितुर्वचननिर्देशात् कैकेय्याः प्रियकारणात्"
},

{
 "chaptno" : "03",
 "slokano" : "025-26_1",
 "spart1" : "तं व्रजन्तं प्रियो भ्राता लक्ष्मणोऽनुजगाम ह",
"spart2" : "स्नेहाद्विनयसम्पन्नः सुमित्रानन्दवर्धनः।  भ्रातरं दयितो भ्रातुः सौभ्रात्रमनुदर्शयन्"
},

{
 "chaptno" : "03",
 "slokano" : "026_2",
 "spart1" : "रामस्य दयिता भार्या",
"spart2" : " नित्यं प्राणसमा हिता"
},

{
 "chaptno" : "03",
 "slokano" : "027",
"spart1" : "जनकस्य कुले जाता देवमायेव निर्मिता। ",
"spart2" : " सर्वलक्षणसम्पन्ना नारीणामुत्तमा वधूः "
},

{
 "chaptno" : "03",
 "slokano" : "028_1",
 "spart1" : " सीताप्यनुगता रामं शशिनं रोहिणी यथा"
},

{
 "chaptno" : "03",
 "slokano" : "028_2-29",
 "spart1" : "पौरैरनुगतोदूरं पित्रा दशरथेन च",
"spart2" : "शृङ्गवेरपुरे सूतं गङ्गाकूले व्यसर्जयत्। गुहमासाद्य धर्मात्मा निषादाधिपतिं  प्रियम् "
},

{
 "chaptno" : "03",
 "slokano" : "030-32_1",
 "spart1" : "गुहेन सहितो रामो लक्ष्मणेन च सीतया ते वनेन वनं गत्वा नदीस्तीर्त्वा बहूदकाः। ",
"spart2" : "चित्रकूटमनुप्राप्य भरद्वाजस्य शासनात्। रम्यमावसथं कृत्वा रममाणा वने त्रयः देवगन्धर्वसङ्काशास्तत्र ते न्यवसन् सुखम्"
},

{
 "chaptno" : "04",
 "slokano" : "032_2-33_1",
 "spart1" : "चित्रकूटं गते रामे पुत्रशोकातुरस्तथा",
"spart2" : "राजा दशरथः स्वर्गं जगाम विलपन् सुतम्"
},

{
 "chaptno" : "04",
 "slokano" : "033_2-34_1",
 "spart1" : "गते तु तस्मिन् भरतो वसिष्ठप्रमुखैर्द्वजैः",
"spart2" : "नियुज्यमानो राज्याय नैच्छद्राज्यं महाबलः."
},

{
 "chaptno" : "04",
 "slokano" : "034_2",
"spart1": " स जगाम वनं वीरो रामपादप्रसादकः"
},

{
 "chaptno" : "04",
 "slokano" : "035",
 "spart1" : "गत्वा तु स महात्मानं रामं सत्यपराक्रमम्",
"spart2" : "अयाचद् भ्रातरं राममार्यभावपुरस्कृतः."
},

{
 "chaptno" : "04",
 "slokano" : "036",
 "spart1" : "त्वमेव राजा धर्मज्ञ इति रामं वचोऽब्रवीत्",
 "spart2" : "रामोऽपि परमोदारः सुमुखः सुमहायशाः"
},

{
 "chaptno" : "04",
 "slokano" : "037_1",
 "spart2" : "न चैच्छत् पितुरादेशाद्राज्यं रामो महाबलः."
},

{
 "chaptno" : "04",
 "slokano" : "037_2-38_1",
 "spart1" : "पादुके चास्य राज्याय न्यासं दत्त्वा पुनः पुनः.",
 "spart2" : "निवर्तयामास ततो भरतं भरताग्रजः"
},

{
 "chaptno" : "04",
 "slokano" : "038_2-39_1",
 "spart1" : "स काममनवाप्यैव रामपादावुपस्पृशन्",
"spart2" : "नन्दिग्रामेऽकरोद्राज्यं  रामागमनकाङ्क्षया"
},

{
 "chaptno" : "04",
 "slokano" : "039_2-40",
 "spart1" : "गते तु भरते श्रीमान् सत्यसन्धो जितेन्द्रियः",
"spart2" : "रामस्तु पुनरालक्ष्य नागरस्य जनस्य च।  तत्रागमनमेकाग्रो दण्डकान् प्रविवेश ह"
},

{
 "chaptno" : "05",
 "slokano" : "041-42_1",
 "spart1" : "प्रविश्य तु महारण्यं रामो राजीवलोचनः",
"spart2" : "विराधं राक्षसं हत्वा शरभङ्गं ददर्श ह।  सुतीक्ष्णं चाप्यगस्त्यं च अगस्त्यभ्रातरं तथा"
},

{
 "chaptno" : "05",
 "slokano" : "042_2-43_1",
 "spart1" : "अगस्त्यवचनाच्चैव जग्राहैन्द्रं शरासनम्",
"spart2" : "खड्गं च परमप्रीतस्तूणी चाक्षयसायकौ"
},

{
 "chaptno" : "05",
 "slokano" : "043_2-44_1",
 "spart1" : "वसतस्तस्य रामस्य वने वनचरैः सह",
"spart2" : "ऋषयोऽभ्यागमन्सर्वे वधायासुररक्षसाम्। "
},

{
 "chaptno" : "05",
 "slokano" : "044_2",
"spart1" : "स तेषां प्रतिशुश्राव राक्षसानां तदा वने"
},

{
 "chaptno" : "05",
 "slokano" : "045",
 "spart1" : "प्रतिज्ञातश्च रामेण वधः संयति रक्षसाम्",
"spart2" : "ऋषीणामग्निकल्पानां दण्डकारण्यवासिनाम्"
},

{
 "chaptno" : "05",
 "slokano" : "046",
 "spart1" : "तेन तत्रैव वसता जनस्थाननिवासिनी",
"spart2" : "विरूपिता शूर्पणखा राक्षसी कामरूपिणी"
},

{
 "chaptno" : "05",
 "slokano" : "047-48_1",
 "spart1" : "ततः शूर्पणखावाक्यादुद्युक्तान् सर्वराक्षसान्",
"spart2" : "खरं त्रिशिरसं चैव दूषणं चैव राक्षसम् निजघान रणे रामस्तेषां चैव पदानुगान्"
},

{
 "chaptno" : "05",
 "slokano" : "048_2-49_1",
 "spart1" : "वने तस्मिन्निवसता जनस्थाननिवासिनाम्",
"spart2" : "रक्षसां निहतान्यासन् सहस्राणि चतुर्दश"
},

{
 "chaptno" : "05",
 "slokano" : "049_2",
 "spart1" : "ततो ज्ञातिवधं श्रुत्वा रावणः क्रोधमूर्छितः"
},

{
 "chaptno" : "05",
 "slokano" : "050",
 "spart1" : "सहायं वरयामास मारीचं नाम राक्षसम्",
 "spart2" : "वार्यमाणः सुबहुशो मारीचेन स रावणः"
},

{
 "chaptno" : "06",
 "slokano" : "051_1",
"spart1" : "न विरोधो बलवता क्षमो रावण तेन ते।  "
},

{
 "chaptno" : "06",
 "slokano" : "051_2-52_1",
"spart1" : "अनादृत्य तु तद्वाक्यं रावणः कालचोदितः ",
"spart2" : "जगाम सहमारीचस्तस्याश्रमपदं तदा"
},

{
 "chaptno" : "06",
 "slokano" : "052_2-53_1",
 "spart1" : "तेन मायाविना दूरमपवाह्य नृपात्मजौ",
"spart2" : "जहार भार्यां रामस्य गृध्रं हत्वा जटायुषम्"
},

{
 "chaptno" : "06",
 "slokano" : "053_2-54_1",
 "spart1" : "गृध्रं च निहतं दृष्ट्वा हृतां श्रुत्वा च मैथिलीम्",
"spart2" : "राघवः शोकसंतप्तो विललापाकुलेन्द्रियः"
},

{
 "chaptno" : "06",
 "slokano" : "054_2-55",
 "spart1" : "ततस्तेनैव शोकेन गृध्रं दग्ध्वा जटायुषम्",
"spart2" : "मार्गमाणो वने सीतां राक्षसं संददर्श ह।  कबन्धं नाम रूपेण विकृतं घोरदर्शनम्"
},

{
 "chaptno" : "06",
 "slokano" : "056",
 "spart1" : "तं निहत्य महाबाहुर्ददाह स्वर्गतश्च सः"
},

{
 "chaptno" : "06",
 "slokano" : "056_3-57_1",
"spart1" : "स चास्य कथयामास शबरीं धर्मचारिणीम्।",
"spart2" : "श्रमणां धर्मनिपुणामभिगच्छेति राघव"
},

{
 "chaptno" : "06",
 "slokano" : "057_2",
 "spart1" : "सोऽभ्यगच्छन् महातेजाः शबरीं शत्रुसूदनः"
},

{
 "chaptno" : "06",
 "slokano" : "058",
"spart1" : "शबर्या पूजितः सम्यग् रामो दशरथात्मजः",
"spart2" : "पम्पातीरे हनुमता सङ्गतो वानरेण ह "
},

{
 "chaptno" : "06",
 "slokano" : "059_1",
"spart1" : "हनुमद्वचनाच्चैव सुग्रीवेण समागतः"
},

{
 "chaptno" : "06",
 "slokano" : "059_2-60_1",
 "spart1" : "सुग्रीवाय च तत्सर्वं शंसद् रामो महावलः",
"spart2" : "आदितस्तद्यथावृत्तं सीतायाश्च विशेषतः"
},

{
 "chaptno" : "07",
 "slokano" : "060_2-61_1",
 "spart1" : "सुग्रीवश्चापि तत्सर्वं श्रुत्वा रामस्य वानरः",
"spart2" : "चकार सख्यं रामेण प्रीतश्चैवाग्निसाक्षिकम्"
},

{
 "chaptno" : "07",
 "slokano" : "061_2-62_1",
 "spart1" : "ततो वानरराजेन वैरानुकथनं प्रति ",
"spart2" : "रामायावेदितं सर्वं प्रणयाद्दुःखितेन च।"
},


{
 "chaptno" : "07",
 "slokano" : "062_2",
